{
  "name": "gatsby-starter",
  "description": "Clean starter for gatsby website, with image optimization and GDPR cookies",
  "version": "0.2.1",
  "author": "Wojciech Kałużny",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.24.73",
