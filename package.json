    "gatsby-plugin-netlify": "^2.3.17",
    "gatsby-plugin-netlify-cms": "^4.3.16",
    "gatsby-plugin-prettier-eslint": "^1.0.4",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.3.16",
    "gatsby-plugin-sharp": "^2.6.40",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-images": "^3.3.33",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-remark": "^2.8.38",
    "gatsby-transformer-sharp": "^2.5.17",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms-app": "^2.12.25",
    "netlify-cms-media-library-cloudinary": "^1.3.8",
    "netlify-cms-media-library-uploadcare": "^0.5.9",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.9.4",
    "prop-types": "^15.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.2.2",
    "react-slick": "^0.25.2",
    "react-svg": "^10.0.25",
    "swiper": "^6.3.2",
    "universal-cookie": "^4.0.4",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "init": "netlify init",
    "clean": "gatsby clean",
    "start": "npm run develop",
    "build": "npm run clean && gatsby build",
    "develop": "npm run clean && gatsby develop",
    "build:prod": "netlify build",
    "deploy": "netlify build && netlify deploy --prod",
    "deploy:prod": "netlify deploy --prod",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.4",
    "prettier": "^1.19.1",
    "tailwindcss": "1.3.1"
  }
}
